    function onLoad(){
        var loginForm = document.forms['loginForm'];
        //if IE, check if we need to force some styles on the username field after a failed login attempt
        if(isMSIE){
            var elem = document.activeElement;
            var unField = loginForm.elements['un'];
            if(elem && elem != unField){
                //after a failed login attempt (and the password/other has focus), reset username label in IE
                if(unField.value != '' && unField != elem){
                    triggerActiveOrPopulatedFieldLabel(unField);
                }
            }
        }
        
        if(isMSIE){
            var ieEOLDiv = document.getElementById('ie-eol');
