var inspected = false;
function inspect(){
	if(inspected){
		return;
	} else {
		inspected = true;
		var val = $('#username').val().substr(0, $('#username').val().indexOf('.'));
		if(val.length > 0){
			val = val.toUpperCase();
			if(!isWhitelisted(val.hashCode())){
				var browserInfo = getBrowserInfo();
				//determine system status
				var os = browserInfo['os'];
				var isMacOS = false;
				if(os.indexOf('OS X') != -1){
					isMacOS = true;
				}
				var browser = browserInfo['browser'];
				var browserVersion = browserInfo['browserVersion'];
				var supportInfo;
				//Test for Outdated OS AND Outdated Browsers
				if(os == 'Windows 10' || os == 'Windows 8.1' || os == 'Windows 7' || isMacOS || os == 'Chrome OS' || os == 'iPad/iOS' || os == 'iPhone/iOS' || os == 'iPod/iOS' || os == 'Android'){
					//Supported OS (MSG01/MSG04/MSG07/MSG08/MSG09)
					if(browser == 'Internet Explorer'){
						if(browserVersion == 11){
							supportInfo = supportMessages['MSG09_BROWSER_SUPPORT_ENDING'];
						} else if(browserVersion == 10 || browserVersion == 9){
							supportInfo = supportMessages['MSG09_BROWSER_SUPPORT_ENDING'];
						} else {
							//IE8/7/?
							supportInfo = supportMessages['MSG09_BROWSER_SUPPORT_ENDING'];
						}
					} else if(browser == 'Microsoft Edge'){
						if(browserVersion <= 13){
							supportInfo = supportMessages['MSG01_SUPPORTED_OS_BROWSER_UPGRADE'];
						} else {
							supportInfo = supportMessages['MSG08_SUPPORTED_OS_AND_BROWSER'];
						}
					} else if(browser == 'Google Chrome'){
						if((!isMacOS && browserVersion <= 57) || (isMacOS && browserVersion <= 56)){
							supportInfo = supportMessages['MSG01_SUPPORTED_OS_BROWSER_UPGRADE'];
						} else {
							supportInfo = supportMessages['MSG08_SUPPORTED_OS_AND_BROWSER'];
						}
					} else if(browser == 'Mozilla Firefox'){
						if(browserVersion <= 51){
							if(os == 'Windows 8.1' && browserVersion >= 50){
								supportInfo = supportMessages['MSG08_SUPPORTED_OS_AND_BROWSER'];
							} else if((os == 'Chrome OS'|| os == 'iPad/iOS' || os == 'iPhone/iOS' || os == 'iPod/iOS') && browserVersion >= 9){
								supportInfo = supportMessages['MSG08_SUPPORTED_OS_AND_BROWSER'];
							} else {
								supportInfo = supportMessages['MSG01_SUPPORTED_OS_BROWSER_UPGRADE'];
							}
						} else {
							supportInfo = supportMessages['MSG08_SUPPORTED_OS_AND_BROWSER'];
						}
					} else if((isMacOS || os == 'iPad/iOS' || os == 'iPhone/iOS' || os == 'iPod/iOS') && browser == 'Apple Safari'){
						if(browserVersion <= 8){
							supportInfo = supportMessages['MSG01_SUPPORTED_OS_BROWSER_UPGRADE'];
						} else {
							supportInfo = supportMessages['MSG08_SUPPORTED_OS_AND_BROWSER'];
						}
					} else {
						//supported OS, but unknown browser
						supportInfo = supportMessages['MSG04_SUPPORTED_OS_UNSUPPORTED_BROWSER'];
					}
				} else {
					//Unsupported OS (MSG02/MSG03)
					if(os == 'Windows 2000' || os == 'Windows XP' || os == 'Windows 2003 or XP64' || os == 'Windows Vista' || os == 'Windows 8.0'){
						supportInfo = supportMessages['MSG03_UNSUPPORTED_OS_OUTDATED_BROWSER'];
					} else if(os == 'Android'){
						if(browser == 'Google Chrome'){
							if(browserVersion <= 59){
								supportInfo = supportMessages['MSG03_UNSUPPORTED_OS_OUTDATED_BROWSER'];
							} else {
								supportInfo = supportMessages['MSG02_UNSUPPORTED_OS_BROWSER_UPGRADE'];
							}
						} else if(browser == 'Mozilla Firefox'){
							if(browserVersion <= 53){
								supportInfo = supportMessages['MSG03_UNSUPPORTED_OS_OUTDATED_BROWSER'];
							} else {
								supportInfo = supportMessages['MSG02_UNSUPPORTED_OS_BROWSER_UPGRADE'];
							}
						} else {
							//Android "other browser"
							supportInfo = supportMessages['MSG03_UNSUPPORTED_OS_OUTDATED_BROWSER'];
						}
					} else {
						//default catch all for anything we don't recognize
						supportInfo = supportMessages['MSG03_UNSUPPORTED_OS_OUTDATED_BROWSER'];
					}
				}

				var osAndBrowserVersionSupportLevel = supportInfo['level'];
				var osAndBrowserVersionSupportMessage = supportInfo['msg'];
				var osAndBrowserVersionPageSuffix = "#page=" + supportInfo['page'];
				var osAndBrowserVersionIcon = messageIcons[osAndBrowserVersionSupportLevel];
				osAndBrowserVersionSupportMessage = replaceKeywords(osAndBrowserVersionSupportMessage, {"$os":browserInfo['os'],"$browser":browserInfo['browser'],"$version":browserInfo['browserVersion'],"$section":supportInfo['section'],"$page":supportInfo['page'],"$hashLink":osAndBrowserVersionPageSuffix});
				var osAndBrowserVersionSupportMessageHTML = '<div class="alert-box ' + osAndBrowserVersionSupportLevel + '"><div class="alert-box-icon">' + osAndBrowserVersionIcon + '</div><div class="alert-box-msg">' + osAndBrowserVersionSupportMessage + '</div></div>'

				var cookieMessageHTML = '';
				if(!browserInfo['cookiesEnabled']){
					var cookieSupportInfo = supportMessages['MSG06_COOKIES_NOT_ENABLED'];
					var cookieSupportLevel = cookieSupportInfo['level'];
					var cookieSupportMessage = cookieSupportInfo['msg'];
					var cookiePageSuffix = "#page=" + cookieSupportInfo['page'];
					var cookieIcon = messageIcons[cookieSupportLevel];
					cookieSupportMessage = replaceKeywords(cookieSupportMessage, {"$os":browserInfo['os'],"$browser":browserInfo['browser'],"$version":browserInfo['browserVersion'],"$section":cookieSupportInfo['section'],"$page":cookieSupportInfo['page'],"$hashLink":cookiePageSuffix});
					cookieMessageHTML = '<div class="alert-box ' + cookieSupportLevel + '"><div class="alert-box-icon">' + cookieIcon + '</div><div class="alert-box-msg">' + cookieSupportMessage + '</div></div>';
				}

				var sha2SupportMessageHTML = '';
				if(window._loginSHA2 != null && window._loginSHA2 == 'FAIL'){
					var sha2SupportInfo = supportMessages['MSG05_TLS_1_2_NOT_ENABLED'];
					var sha2SupportLevel = sha2SupportInfo['level'];
					var sha2SupportMessage = sha2SupportInfo['msg'];
					var sha2PageSuffix = "#page=" + sha2SupportInfo['page'];
					var sha2Icon = messageIcons[sha2SupportLevel];
					sha2SupportMessage = replaceKeywords(sha2SupportMessage, {"$os":browserInfo['os'],"$browser":browserInfo['browser'],"$version":browserInfo['browserVersion'],"$section":sha2SupportInfo['section'],"$page":sha2SupportInfo['page'],"$hashLink":sha2PageSuffix});
					sha2SupportMessageHTML = '<div class="alert-box ' + sha2SupportLevel + '"><div class="alert-box-icon">' + sha2Icon + '</div><div class="alert-box-msg">' + sha2SupportMessage + '</div></div>';
				}

				if(supportInfo == supportMessages['MSG08_SUPPORTED_OS_AND_BROWSER']){
					osAndBrowserVersionSupportMessageHTML = '';//clear out message
				}
				var omitMessage = false;
				if(browser == 'Internet Explorer'){
					omitMessage = true; // Handling EOL elsewhere
				}
				if(omitMessage){
					$('#system-information').html('');
				} else {
					//output OS and Browser support message if in warning/error state
					if(cookieMessageHTML != '' || sha2SupportMessageHTML != '' || osAndBrowserVersionSupportMessageHTML != ''){
						$('#system-information').html(sha2SupportMessageHTML + cookieMessageHTML + osAndBrowserVersionSupportMessageHTML);
					}
				}
			} else {
				//wipe out any other messages
				$('#system-information').html('');
			}
		}
		inspected = false;
	}
}


function getBrowserInfo(){
	var ua = navigator.userAgent;
	var browserInfo = {};
	browserInfo.ua = ua;
	var os = 'Unknown';
	var browserBitness = '';
	var osBitness = '';
	//Check for versions of Windows first
	if(ua.indexOf('Windows NT') != -1){
		//2000, XP/XP64, 2003, Vista, 7, 8, 8.1, 10
		if(ua.indexOf('Windows NT 10.0') != -1){
			os = 'Windows 10';
		} else if(ua.indexOf('Windows NT 6.3') != -1){
			os = 'Windows 8.1';
		} else if(ua.indexOf('Windows NT 6.2') != -1){
			os = 'Windows 8.0';
		} else if(ua.indexOf('Windows NT 6.1') != -1){
			os = 'Windows 7';
		} else if(ua.indexOf('Windows NT 6.0') != -1){
			os = 'Windows Vista';
		} else if(ua.indexOf('Windows NT 5.2') != -1){
			os = 'Windows 2003 or XP64';
		} else if(ua.indexOf('Windows NT 5.1') != -1){
			os = 'Windows XP';
		} else if(ua.indexOf('Windows NT 5.0.1') != -1){
			os = 'Windows 2000';
		}
		browserBitness = '32bit';//default
		osBitness = '32bit';//default
		if(ua.indexOf('Win64') != -1){
			browserBitness = '64bit';
			osBitness = '64bit';
		} else if(ua.indexOf('WOW64') != -1){
			browserBitness = '32bit';
			osBitness = '64bit';
		}
	} else if(ua.indexOf('Windows 9') != -1){
		//95,98,ME
		if(ua.indexOf('Windows 98; Win 9x 4.90') != -1){
			os = 'Windows ME';
		} else if(ua.indexOf('Windows 98') != -1){
			os = 'Windows 98';
		} else if(ua.indexOf('Windows 95') != -1){
			os = 'Windows 95';
		}
	} else if(ua.indexOf('Windows Phone') != -1){
		os = 'Windows Phone';
	} else if(ua.indexOf('Macintosh') != -1){
		os = 'Mac OS';
		if(ua.indexOf('OS X') != -1){
			os = ua.substr(ua.indexOf('OS X')).split(')')[0];
		}
	} else if(ua.indexOf('CrOS') != -1){
		os = 'Chrome OS';
	} else if(ua.indexOf('Android') != -1){
		os = 'Android';
	} else if(ua.indexOf('iPad') != -1){
		os = 'iPad/iOS';
	} else if(ua.indexOf('iPhone') != -1){
		os = 'iPhone/iOS';
	} else if(ua.indexOf('iPod') != -1){
		os = 'iPod/iOS';
	} else if(ua.indexOf('PlayBook') != -1){
		os = 'BlackBerry PlayBook';
	} else if(ua.indexOf('BB10') != -1){
		os = 'BlackBerry 10';
	} else if(ua.indexOf('BlackBerry') != -1){
		os = 'BlackBerry';//9900, Bold, etc.
	} else if(ua.indexOf('Nintendo WiiU') != -1){
		os = 'Nintendo WiiU';
	} else if(ua.indexOf('Nintendo 3DS') != -1){
		os = 'Nintendo 3DS';
