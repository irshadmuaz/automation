		os = 'PlayStation';
		if(ua.indexOf('PlayStation Vita') != -1){
			os = 'PlayStation Vita';
		}
	} else if(ua.indexOf('AQUOSBrowser') != -1){
		os = 'Sharp/Espial DTV';
	} else if(ua.indexOf('SMART-TV') != -1){
		os = 'SMART TV';
	} else if(ua.indexOf('Linux') != -1){
		os = 'Linux';
	}

	browserInfo.os = os;
	browserInfo.browserBitness = browserBitness;
	browserInfo.osBitness = osBitness;
