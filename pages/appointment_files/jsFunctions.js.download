var XML_CHAR_MAP={'<':'&lt;','>':'&gt;','&':'&amp;','"':'&quot;',"'":'&apos;'};function escapeXml(s){return escapeXmlExcludingCDATA(s)}function escapeXmlExcludingCDATA(s){if(s===null||s===undefined){return""}if(typeof s==="string"&&s.indexOf("[CDATA[")===-1){return s.replace(/&(?!(lt|gt|amp|quot|apos);)|[<>"']/g,function(ch){return XML_CHAR_MAP[ch]})}else{return s}}function addElement(xw,elementName,value,attributeLabel,attributeValue){xw.writeStartElement(elementName.toString());xw.writeString(escapeXmlExcludingCDATA(value.toString()));if(attributeLabel!==undefined&&typeof attributeValue!==undefined){xw.writeAttributeString(attributeLabel,attributeValue)}xw.writeEndElement()}function escapeHtml(text){if(text===null||text===undefined){return""}if(typeof text!=="string"){return text}return text.replace(/[\"&'<>]/g,function(a){return{'"':'&quot;','&':'&amp;',"'":'&apos;','<':'&lt;','>':'&gt;'}[a]})}function unescapeHtml(text){if(text===null||text===undefined){return""}if(typeof text!=="string"){return text}return text.replace(/&amp;/g,'&').replace(/&lt;/g,'<').replace(/&gt;/g,'>').replace(/&quot;|&#34;/g,'"').replace(/&#39;|&apos;/g,"'")}function unescapeHtmlForJS(str){return str.replace(/&amp;/g,"&").replace(/&quot;/g,'"').replace(/&39;/g,'/').replace(/&#47;/g,'/').replace(/&#39;|&apos;/g,'\'').replace(/&#x2F;/g,'/').replace(/&#x60;/g,'`').replace(/&#x3D;/g,'=')}function unescapeHtmlForJsonString(text){if(text===null||text===undefined){return""}if(typeof text!=="string"){return text}return text.replace(/&amp;/g,'&').replace(/&lt;/g,'<').replace(/&gt;/g,'>').replace(/&quot;|&#34;/g,'\\"').replace(/&#39;|&apos;/g,"'")}var JS_CHAR_MAP={'&':'\\x26','-':'\\-','\\':'\\\\',"'":"\\x27",'"':'\\x22','/':'\\/'};function escapeJavaScript(s){if(s===null||s===undefined){return""}if(typeof s==="string"){return s.replace(/[&-'"\/\\]/gm,function(ch){return JS_CHAR_MAP[ch]})}else{return s}}