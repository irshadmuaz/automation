function getHost(url){return location.protocol+'//'+location.hostname+(location.port?':'+location.port:'')+url}function workerAjaxCall_GET(url){try{var p=new Parallel(getHost(url));return p.spawn(function(url){var xhr=new XMLHttpRequest;xhr.open('GET',url,false);xhr.send(null);if(xhr.readyState==4){if(xhr.status==200||xhr.status==304||xhr.status==0){if(typeof xhr.response!=='object'&&typeof xhr.response!=='json'){return JSON.parse(xhr.response)}else{return xhr.response}}else{return xhr.status+xhr.responseText}}})}catch(e){console.log(e)}}function workerAjaxCall_POST(url,data){try{data["_WEB_WORKER_URL"]=getHost(url);var p=new Parallel(data);return p.spawn(function(requestData){var xhr=new XMLHttpRequest;xhr.open('POST',requestData._WEB_WORKER_URL,false);xhr.setRequestHeader("Content-type","application/x-www-form-urlencoded");delete requestData._WEB_WORKER_URL;var data="";if(requestData){for(var key in requestData){if(requestData.hasOwnProperty(key)){data+="&"+key+"="+requestData[key]}}if(data){xhr.send(data.substring(1,data.length))}else{xhr.send(null)}}else{xhr.send(null)}if(xhr.readyState==4){if(xhr.status==200||xhr.status==304||xhr.status==0){if(typeof xhr.response!=='object'&&typeof xhr.response!=='json'){try{return JSON.parse(xhr.response)}catch(e){return xhr.response}}else{return xhr.response}}else{return xhr.status+xhr.responseText}}})}catch(e){console.log(e)}}